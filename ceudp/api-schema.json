{
    "paths": {
        "/api/management/login/": {
            "post": {
                "tags": [
                    "用户认证"
                ],
                "description": "获取用户认证信息（Token）\n",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "title": "User",
                            "type": "object",
                            "properties": {
                                "username": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },

        "v1/user/profile/": {
            "get": {
                "tags": [
                    "v1"
                ],
                "description": "用户profile\n",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "title": "User",
                            "type": "object",
                            "properties": {
                                "username": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "phone_number": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }

    }
}